<SCRIPT LANGUAGE="JavaScript" SRC="../../../expandcollapse.js"></SCRIPT>

<script type="text/javascript">
var i, j, digiclock
digiclock = "00:00:00";
j = 0;
function prog()
{
  document.getElementById("d1").innerHTML="";	
  document.getElementById("d2").style.width=0;  
  i=0;
  progBar(); 
}

function progBar()
{
	if (i<=(500)) {
		if (i>50) {
			document.getElementById("d1").innerHTML=parseInt(i/5)+"%";
		}
		document.getElementById("d2").style.width=i+"px";
		var j=0;		
		while (j<=100)
			j++;  
		setTimeout("progBar();", 1000); 
		i++;   
	}
	else {
		if (document.getElementById('prepage').style.display != 'none') {
			document.getElementById('prepage').style.display='none';
			alert('Se excedió el tiempo máximo permitido para la consulta. Reitente en instantes por favor.');
		}
	}
}

function timer()
{
    var digiformat = "";
	if(j>3599)
	{
		var H = Math.floor(j/3600);
	}
	else
	{
		var H = 0;
	}
	
	var M = j - (H*3600)
	
	if(M>59)
	{
		M = Math.floor(M/60)
	}
	else
	{
		M = 0
	}	
	var S = j - (M*60)	
	if(H<10)
	{
		H = "0"+H;
	}
	if(M<10)
	{
		M = "0"+M;
	}
	if(S<10)
	{
		S = "0"+S;
	}
	document.getElementById('time').innerHTML = H+":"+M+":"+S;
    setTimeout('timer()', 1000);
    j++;
}
</script>
